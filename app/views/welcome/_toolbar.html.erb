	$('.toggle-menu').hover(function(e){
			console.log('be clickin');
			e.preventDefault();
			$(this).parent().siblings().children('.toggle-menu').removeClass('active').next().slideUp();
			$(this).toggleClass('active').next().slideToggle();
			e.stopPropagation();
	});
</script>

